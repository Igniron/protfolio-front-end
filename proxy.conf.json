{
    "/api/*": {
      "target": "http://localhost:8080",
      "secure": false,
      "changeOrigin": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
      }
    }
  }